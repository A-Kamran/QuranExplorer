    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://quranontology.com/Resource/AllyTo -->

    <owl:ObjectProperty rdf:about="&qur;AllyTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">حليف ل</rdfs:label>
        <rdfs:label xml:lang="en">Ally To</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;ContemporaryWith"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/AncestorOf -->

    <owl:ObjectProperty rdf:about="&qur;AncestorOf">
        <rdfs:label xml:lang="ar">سلف ل</rdfs:label>
        <rdfs:label xml:lang="en">Ancestor Of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/BelongsInPeriod -->

    <owl:ObjectProperty rdf:about="&qur;BelongsInPeriod">
		<rdfs:label xml:lang="ar">ينتمي إلى الفترة</rdfs:label>
        <rdfs:label xml:lang="en">Belongs In Period</rdfs:label>
	    <owl:inverseOf rdf:resource="&qur;PeriodHasPerson"/>	
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Period"/>
	 </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/BondWith -->

    <owl:ObjectProperty rdf:about="&qur;BondWith">
        <rdfs:label xml:lang="ar">له علاقة مع</rdfs:label>
        <rdfs:label xml:lang="en">Bond With</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/ContainsLocation -->

    <owl:ObjectProperty rdf:about="&qur;ContainsLocation">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="ar">يحتوي على الموقع</rdfs:label>
        <rdfs:label xml:lang="en">Contains Location</rdfs:label>
        <rdfs:domain rdf:resource="&qur;Location"/>
        <rdfs:range rdf:resource="&qur;Location"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/ContainsMentionOf -->

    <owl:ObjectProperty rdf:about="&qur;ContainsMentionOf">
        <rdfs:label xml:lang="ar">يحتوي ذكر</rdfs:label>
        <rdfs:label xml:lang="en">Contains mention of</rdfs:label>
        <rdfs:domain rdf:resource="&qur;Verse"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Location"/>
                    <rdf:Description rdf:about="&qur;Topic"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/ContemporaryWith -->

    <owl:ObjectProperty rdf:about="&qur;ContemporaryWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">عايش</rdfs:label>
        <rdfs:label xml:lang="en">Contemporary With</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;BondWith"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/DiscussTopic -->

    <owl:ObjectProperty rdf:about="&qur;DiscussTopic">
        <rdfs:label xml:lang="ar">يناقش الموضوع</rdfs:label>
        <rdfs:label xml:lang="en">DiscussTopic</rdfs:label>
        <rdfs:comment xml:lang="en">Indicate for a verse the topic discussed</rdfs:comment>
        <rdfs:comment xml:lang="en">تشير إلى الموضوع المناقش في الأية</rdfs:comment>
        <rdfs:range rdf:resource="&qur;Topic"/>
        <rdfs:domain rdf:resource="&qur;Verse"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/DiscussedIn -->

    <owl:ObjectProperty rdf:about="&qur;DiscussedIn">
        <rdfs:label xml:lang="ar">نوقش في</rdfs:label>
        <rdfs:label xml:lang="en">DiscussedIn</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Indicate for a topic the verse where he is being discussed</rdfs:comment>
        <owl:inverseOf rdf:resource="&qur;DiscussTopic"/>
        <rdfs:domain rdf:resource="&qur;Topic"/>
        <rdfs:range rdf:resource="&qur;Verse"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/EnemyWith -->

    <owl:ObjectProperty rdf:about="&qur;EnemyWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">عدو ل</rdfs:label>
        <rdfs:label xml:lang="en">Enemy With</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;ContemporaryWith"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/EventAtPeriod -->

    <owl:ObjectProperty rdf:about="&qur;EventAtPeriod">
        <rdfs:label xml:lang="ar">حدث في الفترة</rdfs:label>
        <rdfs:label xml:lang="en">Event at period</rdfs:label>
        <rdfs:inverseOf rdf:resource="&qur;PeriodHasEvent"/>	
        <rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;Period"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/EventAtPlace -->

    <owl:ObjectProperty rdf:about="&qur;EventAtPlace">
        <owl:inverseOf rdf:resource="&qur;PlaceHasEvent"/>
		<rdfs:label xml:lang="ar">حدث في المكان</rdfs:label>
        <rdfs:label xml:lang="en">Event At Place</rdfs:label>
        <rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;Place"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/FamilyBondWith -->

    <owl:ObjectProperty rdf:about="&qur;FamilyBondWith">
        <rdfs:label xml:lang="ar">له علاقة عائلية مع</rdfs:label>
        <rdfs:label xml:lang="en">Family Bond with</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;BondWith"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/Follows -->

    <owl:ObjectProperty rdf:about="&qur;Follows">
	    <owl:inverseOf rdf:resource="&qur;IsFollowedBy"/>
		<rdfs:label xml:lang="ar">يتبع</rdfs:label>
        <rdfs:label xml:lang="en">Follows</rdfs:label>		
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Event"/>
                    <rdf:Description rdf:about="&qur;Period"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Event"/>
                    <rdf:Description rdf:about="&qur;Period"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
	</owl:ObjectProperty>
    
	<owl:ObjectProperty rdf:about="&qur;IsCausedBy">
        <rdfs:label xml:lang="ar">نجم عن</rdfs:label>
        <rdfs:label xml:lang="en">Is Caused By</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;Follows"/>
		<owl:inverseOf rdf:resource="&qur;Caused"/>
		<rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;Event"/>
    </owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&qur;Caused">
        <rdfs:label xml:lang="ar">تسبب في</rdfs:label>
        <rdfs:label xml:lang="en">Caused</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;IsFollowedBy"/>
		<owl:inverseOf rdf:resource="&qur;IsCausedBy"/>
				<rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;Event"/>
    </owl:ObjectProperty>


    <!-- http://quranontology.com/Resource/FriendOf -->

    <owl:ObjectProperty rdf:about="&qur;FriendOf">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">صديق ل</rdfs:label>
        <rdfs:label xml:lang="en">Friend Of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;ContemporaryWith"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/GrandParentOf -->

    <owl:ObjectProperty rdf:about="&qur;GrandParentOf">
        <rdfs:label xml:lang="ar">جد ل</rdfs:label>
        <rdfs:label xml:lang="en">Grandparent Of</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;AncestorOf"/>
        <owl:inverseOf rdf:resource="&qur;HasGrandParent"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasAncestor -->

    <owl:ObjectProperty rdf:about="&qur;HasAncestor">
        <rdfs:label xml:lang="ar">له سلف</rdfs:label>
        <rdfs:label xml:lang="en">Has Ancestor</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;AncestorOf"/>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasChild -->

    <owl:ObjectProperty rdf:about="&qur;HasChild">
        <rdfs:label xml:lang="ar">له إبن</rdfs:label>
        <rdfs:label xml:lang="en">Has Child</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <owl:inverseOf rdf:resource="&qur;HasParent"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasGrandParent -->

    <owl:ObjectProperty rdf:about="&qur;HasGrandParent">
        <rdfs:label xml:lang="ar">حفيد ل</rdfs:label>
        <rdfs:label xml:lang="en">Has Grandparent</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;HasAncestor"/>
        <rdfs:range rdf:resource="&qur;Person"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasMember -->

    <owl:ObjectProperty rdf:about="&qur;HasMember">
        <rdfs:label xml:lang="ar">يضم عضو</rdfs:label>
        <rdfs:label xml:lang="en">Has Member</rdfs:label>
        <rdfs:domain rdf:resource="&qur;GroupOfPeople"/>
        <owl:inverseOf rdf:resource="&qur;MemberOf"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasParent -->

    <owl:ObjectProperty rdf:about="&qur;HasParent">
        <rdfs:label xml:lang="ar">إبن ل</rdfs:label>
        <rdfs:label xml:lang="en">Has Parent</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasPart -->

    <owl:ObjectProperty rdf:about="&qur;HasPart">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="ar">يحتوي على الجزء</rdfs:label>
        <rdfs:label xml:lang="en">HasPart</rdfs:label>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Verse"/>
                    <rdf:Description rdf:about="&qur;Word"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Chapter"/>
                    <rdf:Description rdf:about="&qur;Verse"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasSibling -->

    <owl:ObjectProperty rdf:about="&qur;HasSibling">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">إخوة</rdfs:label>
        <rdfs:label xml:lang="en">HasSibling</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasSpouse -->

    <owl:ObjectProperty rdf:about="&qur;HasSpouse">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="ar">زوج</rdfs:label>
        <rdfs:label xml:lang="en">HasSpouse</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&qur;FamilyBondWith"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
        <rdfs:range rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/HasSubTopic -->

    <owl:ObjectProperty rdf:about="&qur;HasSubTopic">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="ar">يحتوي على موضوع فرعي</rdfs:label>
        <rdfs:label xml:lang="en">HasSubTopic</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;SubTopicOf"/>
        <rdfs:range rdf:resource="&qur;Topic"/>
        <rdfs:domain rdf:resource="&qur;Topic"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/Involves -->

    <owl:ObjectProperty rdf:about="&qur;Involves">
        <owl:inverseOf rdf:resource="&qur;IsRelatedInEvent"/>
		<rdfs:label xml:lang="ar">يتضمن</rdfs:label>
        <rdfs:label xml:lang="en">Involves</rdfs:label>
		<rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;LivingCreation"/>
    </owl:ObjectProperty>
    

    <!-- http://quranontology.com/Resource/IsFollowedBy -->

    <owl:ObjectProperty rdf:about="&qur;IsFollowedBy">
        <owl:inverseOf rdf:resource="&qur;Follows"/>
		<rdfs:label xml:lang="ar">متبوع ب</rdfs:label>
        <rdfs:label xml:lang="en">Is Followed By</rdfs:label>
		<rdfs:domain rdf:resource="&qur;Event"/>
        <rdfs:range rdf:resource="&qur;Event"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/IsPartOf -->

    <owl:ObjectProperty rdf:about="&qur;IsPartOf">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="ar">جزء من</rdfs:label>
        <rdfs:label xml:lang="en">IsPartOf</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;HasPart"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Pronoun"/>
                    <rdf:Description rdf:about="&qur;Verse"/>
                    <rdf:Description rdf:about="&qur;Word"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;Chapter"/>
                    <rdf:Description rdf:about="&qur;Verse"/>
                    <rdf:Description rdf:about="&qur;Word"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/IsRelatedInEvent -->

    <owl:ObjectProperty rdf:about="&qur;IsRelatedInEvent">
	    <owl:inverseOf rdf:resource="&qur;Involves"/>
		<rdfs:label xml:lang="ar">له تدخل في</rdfs:label>
        <rdfs:label xml:lang="en">Is Related In Event</rdfs:label>
		<rdfs:domain rdf:resource="&qur;LivingCreation"/>
        <rdfs:range rdf:resource="&qur;Event"/>
	</owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/LivedIn -->

    <owl:ObjectProperty rdf:about="&qur;LivedIn">
        <rdfs:label xml:lang="ar">عاش في</rdfs:label>
        <rdfs:label xml:lang="en">Lived In</rdfs:label>
        <rdfs:range rdf:resource="&qur;GeographicalLocation"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;GroupOfPeople"/>
                    <rdf:Description rdf:about="&qur;Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/LocatedIn -->

    <owl:ObjectProperty rdf:about="&qur;LocatedIn">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="ar">يقع في</rdfs:label>
        <rdfs:label xml:lang="en">LocatedIn</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;ContainsLocation"/>
        <rdfs:range rdf:resource="&qur;Location"/>
        <rdfs:domain rdf:resource="&qur;Location"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/MemberOf -->

    <owl:ObjectProperty rdf:about="&qur;MemberOf">
        <rdfs:label xml:lang="ar">ينتمي إلى</rdfs:label>
        <rdfs:label xml:lang="en">Member Of</rdfs:label>
        <rdfs:range rdf:resource="&qur;GroupOfPeople"/>
        <rdfs:domain rdf:resource="&qur;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/MentionedIn -->

    <owl:ObjectProperty rdf:about="&qur;MentionedIn">
        <rdfs:label xml:lang="ar">ذكر في</rdfs:label>
        <rdfs:label xml:lang="en">Mentioned In</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;ContainsMentionOf"/>
        <rdfs:range rdf:resource="&qur;Verse"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&qur;LivingCreation"/>
                    <rdf:Description rdf:about="&qur;Location"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/PeriodHasEvent -->

    <owl:ObjectProperty rdf:about="&qur;PeriodHasEvent">
        <rdfs:label xml:lang="ar">تحتوي على الحدث</rdfs:label>
        <rdfs:label xml:lang="en">Period Has Event</rdfs:label>
        <owl:inverseOf rdf:resource="&qur;EventAtPeriod"/>
		<rdfs:domain rdf:resource="&qur;Period"/>
        <rdfs:range rdf:resource="&qur;Event"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/PeriodHasPerson -->

    <owl:ObjectProperty rdf:about="&qur;PeriodHasPerson">
        <owl:inverseOf rdf:resource="&qur;BelongsInPeriod"/>
		 <rdfs:label xml:lang="ar">تضم الشخص</rdfs:label>
        <rdfs:label xml:lang="en">Period Has Person</rdfs:label>
		<rdfs:domain rdf:resource="&qur;Period"/>
        <rdfs:range rdf:resource="&qur;Human"/>
    </owl:ObjectProperty>
    


    <!-- http://quranontology.com/Resource/PlaceHasEvent -->

    <owl:ObjectProperty rdf:about="&qur;PlaceHasEvent">
	<owl:inverseOf rdf:resource="&qur;EventAtPlace"/>
		 <rdfs:label xml:lang="ar">يضم الحدث</rdfs:label>
        <rdfs:label xml:lang="en">Place Has event</rdfs:label>
		<rdfs:domain rdf:resource="&qur;Place"/>
        <rdfs:range rdf:resource="&qur;Event"/>
    </owl:ObjectProperty>


    <!-- http://quranontology.com/Resource/ReferTo -->

    <owl:ObjectProperty rdf:about="&qur;ReferTo">
        <rdfs:label xml:lang="ar">يعود على</rdfs:label>
        <rdfs:label xml:lang="en">ReferTo</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">The relation between the word pronoun and its reference</rdfs:comment>
        <rdfs:domain rdf:resource="&qur;Pronoun"/>
        <rdfs:range rdf:resource="&qur;ReferenceObject"/>
